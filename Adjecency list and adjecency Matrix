#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n,m,i;
    cin>>n>>m;
    // int adj[n+1][m+1]; //if the graph is 0 based just write adj[n][m]
     //matrix meathod
//     

vector<int> adj[n+1];

for(i=0;i<n+1;i++)
{
    int u,v;                     //u and v representing the nodes in edge  in undirected graph
                                  //if graph is directed from u-->v then just use adj[u].push_back(v); and not adj[v].push_back(u);
    cin>>u>>v;
    adj[u].push_back(v);             //o(2E)  undirected graph and o(E)  directed graph
    adj[v].push_back(u);
    
    
    
}  

